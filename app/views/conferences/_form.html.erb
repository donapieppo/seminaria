<%= dm_form_for @conference do |f| %>  
  <%= f.input :title %>
  <%= f.input :abstract, input_html: { cols: 50, rows: 5 } %>
  <%= f.input :committee %>  
  <%= f.input :start_date, as: :date, html5: true %>  
  <%= f.input :end_date, as: :date, html5: true %>  
  <%= f.input :url, placeholder: "https://" %>  
  <%= f.submit %>  
<% end %>


